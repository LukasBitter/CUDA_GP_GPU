/*--------------------------------------*\
 |*		Imported	 	*|
 \*-------------------------------------*/

extern bool useHisto();

/*----------------------------------------------------------------------*\
 |*			Implementation 					*|
 \*---------------------------------------------------------------------*/

/*--------------------------------------*\
 |*		Constructor		*|
 \*-------------------------------------*/

TestHisto::TestHisto(int deviceId)
    {
    this->deviceId=deviceId;

    TEST_ADD(TestHisto::testAdd);
    }

/*--------------------------------------*\
 |*		Methodes		*|
 \*-------------------------------------*/




void TestHisto::testAdd(void)
    {
    TEST_ASSERT(useHisto() == true);
    }

